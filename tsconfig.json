{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"rootDir": "./src",
		"noEmit": true,
		//"outDir": "./dist",
		"declaration": true,
		"plugins": [
			{
				"transform": "ts-nameof",
				"type": "raw"
			}
		]
	}
}